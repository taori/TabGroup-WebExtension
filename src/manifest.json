{
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"browser_action": {
		"browser_style": true,
		"default_title": "TabGroups",
		"default_popup": "tabGroups.html",
		"theme_icons": [
			{
				"dark": "content/ic_tab_black_24px.svg",
				"light": "content/ic_tab_white_24px.svg",
				"size": 24
			}
		]
	},
	"description": "type 'tg' to use tab group commands.",
	"homepage_url": "https://github.com/taori/TabGroup-WebExtension",
	"omnibox": { "keyword": "tg" },
	"manifest_version": 2,
	"name": "TabGroups",
	"permissions": [
		"tabs",
		"storage"
	],
	"version": "0.9"
}